    <body>
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <h2>Cats Table</h2>
                    <p>All chill bros</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 button-list">
                    <button class="btn btn-default" id="list_btn">List Cats</button>
                    <button class="btn btn-primary">Add Cats</button>
                    <button class="btn btn-warning" disabled="disabled">Edit Cats</button>
                    <button class="btn btn-danger" disabled="disabled">Delete Cats</button>
                </div>
            </div>
            <table class="table table-striped" id="cats_table">
                <tr>
                    <th scope="col">Cat ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Age</th>
                </tr>
                
            </table>
        </div>
    </body>

